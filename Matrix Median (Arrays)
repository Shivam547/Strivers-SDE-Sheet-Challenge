#include<bits/stdc++.h>
int getMedian(vector<vector<int>> &matrix)
{
    vector<int>v;
    for(int i=0;i<matrix.size();i++){
        for(int j=0;j<matrix[0].size();j++){
            v.push_back(matrix[i][j]);
        }
    }
    sort(v.begin(), v.end());
    int mid=v.size()/2;
    return v[mid];

}
